{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "object-literal-sort-keys": false,
        "no-console": false,
        "variable-name": [true, "allow-leading-underscore"],
        "max-line-length": [true, 180]
    },
    "linterOptions": {
        "exclude": [
            "tests",
            "src/**/*.json"
        ]
    }
}